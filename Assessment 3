#include<iostream>
using namespace std;

class vehicle {
public:
void vehicle(){
  cout<<"I am a vehicle"<<endl;
}
};

class.TwoWheeler:public vehicle{
public:
void TwoWheeler(){
  cout<<"I am a twoWheeler"<<endl;
}
};

class car:public TwoWheeler {
public:
void car(){
  cout<<"I am a car"<<endl;

int main(){
  car myCar;

myCar.car();
myCar.TwoWheeler();
myCar.vehicle();

return 0;
}
